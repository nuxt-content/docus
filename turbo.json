{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/dev",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["packages/docs-theme/.output/**", "package/docs-theme/.nuxt/**", "packages/base/.output/**", "package/base/.nuxt/**", "package/github/dist"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "release": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "release:edge": {
      "dependsOn": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "prepare": {
      "cache": false
    }
  }
}
