{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/dev",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".output/**", ".nuxt/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "release": {
      "dependsOn": ["^build"]
    },
    "release:edge": {
      "dependsOn": []
    },
    "dev": {
      "cache": false
    },
    "prepare": {
      "cache": false
    }
  }
}
